                   Foreground="#FFFFFF" FontWeight="DemiBold" Width="49" Grid.Column="2" Grid.RowSpan="2"/>
        <TextBlock HorizontalAlignment="Left" Margin="44,20,0,0" Grid.Row="8" TextWrapping="Wrap" Text="Паспорт" VerticalAlignment="Top"
                   Foreground="#FFFFFF" FontWeight="DemiBold" Width="49" Grid.Column="2"/>
        <TextBox Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="103,11,0,0" Grid.Row="2" TextWrapping="Wrap"  Foreground="#FFFFFF" Text="" BorderBrush="#FFFFFF" VerticalAlignment="Top" Width="219" Grid.Column="2"/>
        <TextBox Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="103,11,0,0" Grid.Row="5" TextWrapping="Wrap"  Foreground="#FFFFFF" Text="" BorderBrush="#FFFFFF" VerticalAlignment="Top" Width="219" Grid.Column="2"/>
        <TextBox Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="103,32,0,0" Grid.Row="6" TextWrapping="Wrap"  Foreground="#FFFFFF" Text="" BorderBrush="#FFFFFF" VerticalAlignment="Top" Width="219" Grid.RowSpan="2" Grid.Column="2"/>
        <TextBox Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="103,11,0,0" Grid.Row="8" TextWrapping="Wrap"  Foreground="#FFFFFF" Text="" BorderBrush="#FFFFFF" VerticalAlignment="Top" Width="219" Grid.Column="2"/>
        <TextBox Grid.ColumnSpan="3" HorizontalAlignment="Left" Margin="103,32,0,0" Grid.Row="3" TextWrapping="Wrap"  Foreground="#FFFFFF" Text="" BorderBrush="#FFFFFF" VerticalAlignment="Top" Width="219" Grid.RowSpan="2" Grid.Column="2"/>
        <Rectangle HorizontalAlignment="Left" Height="450" Fill="#651FFF" VerticalAlignment="Center" Width="140" RadiusX="40.042" RadiusY="40.042" Margin="43,0,0,0" Grid.ColumnSpan="2" Grid.RowSpan="11"/>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

    </Grid>
</Page>
